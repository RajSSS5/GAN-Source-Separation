{
    "dataset": {
        "in_memory_percentage": 1,
	"extract_voice_percentage": 0.5,
        "path": "data/MUSDB/",
        "sample_rate": 16000,
        "type": "musdb18"
    },
    "model": {
        "condition_encoding": "binary",
        "dilations": 9,
        "filters": {
            "lengths": {
                "res": 3,
                "final": [3, 3],
                "skip": 1
            },
            "depths": {
                "res": 64,
                "skip": 64,
                "final": [2048, 256]
            }
        },
        "num_stacks": 4,
        "target_field_length": 1601,
        "target_padding": 1
    },
    "optimizer": {
        "decay": 0.0,
        "epsilon": 1e-08,
        "lr": 0.001,
        "momentum": 0.9,
        "type": "adam"
    },
    "training": {
        "batch_size": 10,
        "early_stopping_patience": 16,
        "loss": {
            "out_1": {
                "l1": 1,
                "l2": 0,
                "weight": 1
            },
            "out_2": {
                "l1": 1,
                "l2": 0,
                "weight": -0.05
            }
        },
        "num_epochs": 250,
        "num_steps_test": 500,
        "num_steps_train": 2000,
        "path": "sessions/002",
        "verbosity": 1
    }
}
